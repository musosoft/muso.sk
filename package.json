{
  "name": "muso.sk",
  "description": "Gatsby, Netlify CMS and TailwindCSS project",
  "version": "1.0.1",
  "author": "muso.sk",
  "repository": "https://github.com/musosoft/muso.sk",
  "dependencies": {
    "@fontsource/poppins": "^5.0.13",
    "@headlessui/react": "^1.7.18",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "decap-cms-app": "^3.1.6",
    "gatsby": "^5.13.3",
    "gatsby-adapter-netlify": "^1.1.3",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-remark-copy-linked-files": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-remark-relative-images": "^2.0.5",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@netlify/plugin-gatsby": "^3.8.0",
    "autoprefixer": "^10.4.19",
    "gatsby-plugin-postcss": "^6.13.1",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "postinstall": "gatsby telemetry --disable",
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "build": "yarn clean && gatsby build",
    "format": "prettier --write \"{gatsby-*.js,src/**/*.js}\"",
    "reinstall": "run-script-os",
    "reinstall:nix": "yarn run clean && rm -rf node_modules/ && yarn cache clean && rm -rf yarn.lock && yarn install --force && yarn cache verify && yarn audit fix",
    "reinstall:windows": "yarn run clean && rd /s /q node_modules && yarn cache clean && del yarn.lock && yarn install --force && yarn cache verify && yarn audit fix",
    "upgrade": "run-script-os",
    "upgrade:macos": "brew upgrade node && yarn global add yarn@latest && yarn upgrade-interactive && yarn upgrade",
    "upgrade:windows": "powershell Start-Process powershell -Verb runAs -ArgumentList 'choco upgrade nodejs' && yarn global add yarn@latest && yarn upgrade-interactive && yarn upgrade"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
