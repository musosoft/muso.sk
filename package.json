  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "yarn run develop",
    "build": "yarn run clean && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:clean": "yarn run clean && rm -rf node_modules/ && yarn cache clean && rm -rf yarn.lock && yarn && yarn run build:tailwind",
    "build:tailwind": "tailwind build src/styles/global.css -o src/styles/tailwind.css"
  },
  "browserslist": {
    "production": [
      ">0.23%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4"
  },
  "resolutions": {
    "graphql": "^15.4.0",
    "graphql-compose": "^7.25.0",
    "webpack": "^5.24.2"
