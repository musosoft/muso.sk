{
  "name": "muso.sk",
  "description": "Gatsby, Netlify CMS and TailwindCSS project",
  "version": "1.0.0",
  "author": "muso.sk",
  "repository": "https://github.com/musosoft/muso.sk",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "dependencies": {
    "@fontsource/poppins": "^4.2.2",
    "@headlessui/react": "^1.0.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "gatsby": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-netlify": "^3.3.0",
    "gatsby-plugin-netlify-cms": "^5.3.0",
    "gatsby-plugin-purgecss": "^6.0.1",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-remark-copy-linked-files": "^4.0.0",
    "gatsby-remark-images": "^5.0.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "netlify-cms-app": "^2.14.45",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "gatsby-plugin-postcss": "^4.3.0",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1"
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "npm run develop && watch:tailwind",
    "build": "npm run clean && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "install:clean": "npm run clean && rm -rf node_modules/ && npm cache clean --force && rm -rf package-lock.json && npm install --legacy-peer-deps",
    "build:tailwind": "NODE_ENV=production postcss src/styles/global.css -o src/styles/tailwind.css",
    "watch:tailwind": "NODE_ENV=development postcss src/styles/global.css -o src/styles/tailwind.css -w"
  },
  "browserslist": {
    "production": [
      ">0.23%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
