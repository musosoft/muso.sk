{
  "name": "muso.sk",
  "description": "Gatsby, Netlify CMS and TailwindCSS project",
  "version": "1.0.0",
  "author": "muso.sk",
  "repository": "https://github.com/musosoft/muso.sk",
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^10.0.4",
    "gatsby": "^2.20.35",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-netlify": "^2.2.4",
    "gatsby-plugin-netlify-cms": "^4.2.5",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-remark-copy-linked-files": "^2.2.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^2.4.7",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms-app": "^2.9.6",
    "postcss": "^8.1.9",
    "prop-types": "^15.6.0",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.1.0",
    "tailwindcss": "^2.0.1",
    "uuid": "^7.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "yarn run develop",
    "build": "yarn run clean && gatsby build",
    "develop": "yarn run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:clean": "rm -rf node_modules/ && yarn cache clean && rm -rf package-lock.json && yarn && yarn run build:tailwind && yarn start",
    "build:tailwind": "tailwind build src/styles/global.css -o src/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  }
}
