{
  "name": "muso.sk",
  "description": "Gatsby, Netlify CMS and TailwindCSS project",
  "version": "1.0.0",
  "author": "muso.sk",
  "repository": "https://github.com/musosoft/muso.sk",
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.2.1",
    "gatsby": "^2.30.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-netlify": "^2.9.0",
    "gatsby-plugin-netlify-cms": "^4.8.0",
    "gatsby-plugin-postcss": "^3.5.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-remark-copy-linked-files": "^2.8.0",
    "gatsby-remark-images": "^3.9.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.0",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "^0.11.6",
    "netlify-cms-app": "^2.14.10",
    "postcss": "^8.2.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "tailwindcss": "^2.0.2",
    "uuid": "^8.3.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "yarn run develop",
    "build": "yarn run clean && gatsby build",
    "develop": "yarn run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:clean": "rm -rf node_modules/ && yarn cache clean && rm -rf yarn.lock && yarn && yarn run build:tailwind && yarn start",
    "build:tailwind": "tailwind build src/styles/global.css -o src/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.23%",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
